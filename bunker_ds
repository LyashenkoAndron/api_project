        self.CARDS = ["–£–¥–∞–ª—è–µ—Ç –≤—Å–µ –ø—Ä–æ—Ñ–µ—Å—Å–∏–∏", "–ó–∞–º–µ–Ω—è–µ—Ç –≤—Å–µ –ø—Ä–æ—Ñ–µ—Å—Å–∏–∏", "–ó–∞–º–µ–Ω—è–µ—Ç —Ö–æ–±–±–∏ —ç—Ç–æ–º—É –∏–≥—Ä–æ–∫—É", "–ó–∞–º–µ–Ω—è–µ—Ç —Å–æ—Å—Ç–æ—è–Ω–∏–µ –∑–¥–æ—Ä–æ–≤—å—è —ç—Ç–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–ó–∞–º–µ–Ω—è–µ—Ç —Ö–∞—Ä–∞–∫—Ç–µ—Ä —ç—Ç–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–î–æ–±–∞–≤–ª—è–µ—Ç —ç—Ç–æ–º—É –∏–≥—Ä–æ–∫—É –≤—Ç–æ—Ä–æ–π –±–∞–≥–∞–∂", "–ó–∞–º–µ–Ω—è–µ—Ç –±–∞–≥–∞–∂ —ç—Ç–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–î–æ–±–∞–≤–ª—è–µ—Ç —ç—Ç–æ–º—É –∏–≥—Ä–æ–∫—É –≤—Ç–æ—Ä–æ–µ —Ö–æ–±–±–∏", "–ó–∞–º–µ–Ω—è–µ—Ç –∑–¥–æ—Ä–æ–≤—å–µ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ –∏–¥–µ–∞–ª—å–Ω–æ–µ", "–ó–Ω–∞–µ—Ç, —á—Ç–æ —Ä—è–¥–æ–º –µ—Å—Ç—å –±—É–Ω–∫–µ—Ä —Å –±–æ—Ç–∞–Ω–∏–∫–æ–º –∏ —Ö–∏–º–∏–∫–æ–º", "–ó–Ω–∞–µ—Ç –≥–¥–µ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è —è—â–∏–∫ —Å –æ—Ä—É–∂–∏–µ–º", "–ú–µ–Ω—è–µ—Ç—Å—è —Ö–æ–±–±–∏ —Å –≤—ã–±—Ä–∞–Ω–Ω—ã–º –∏–≥—Ä–æ–∫–æ–º", "–†—è–¥–æ–º–∏ –µ—Å—Ç—å –≤–æ–¥–æ–æ—á–∏—Å—Ç–∏—Ç–µ–ª—å–Ω–∞—è —Å—Ç–∞–Ω—Ü–∏—è", "–†—è–¥–æ–º –µ—Å—Ç—å —Ä–∞–±–æ—Ç–∞—é—â–∞—è —ç–ª–µ–∫—Ç—Ä–æ—Å—Ç–∞–Ω—Ü–∏—è", "–ú–µ–Ω—è–µ—Ç—Å—è –∑–¥–æ—Ä–æ–≤—å–µ–º —Å –≤—ã–±—Ä–∞–Ω–Ω—ã–º –∏–≥—Ä–æ–∫–æ–º", "–ú–µ–Ω—è–µ—Ç—Å—è –ø—Ä–æ—Ñ–µ—Å—Å–∏–µ–π —Å –≤—ã–±—Ä–∞–Ω–Ω—ã–º –∏–≥—Ä–æ–∫–æ–º", "–í–æ–∑–≤—Ä–∞—â–∞–µ—Ç –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –≤ –∏–≥—Ä—É", "–£–±–∏–≤–∞–µ—Ç –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–£–≤–µ–ª–∏—á–∏–≤–∞–µ—Ç –º–µ—Å—Ç–æ –≤ –±—É–Ω–∫–µ—Ä–µ –Ω–∞ 1", "–£–º–µ–Ω—å—à–∞–µ—Ç –º–µ—Å—Ç–æ –≤ –±—É–Ω–∫–µ—Ä–µ –Ω–∞ 1", "–ó–∞–º–µ–Ω—è–µ—Ç —Ñ–æ–±–∏—é –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ –±–æ—è–∑–Ω—å –∑–µ–ª—ë–Ω–æ–≥–æ —Ü–≤–µ—Ç–∞", "–ú–µ–Ω—è–µ—Ç—Å—è —Ñ–æ–±–∏–µ–π —Å –≤—ã–±—Ä–∞–Ω–Ω—ã–º –∏–≥—Ä–æ–∫–æ–º", "–ó–∞–º–µ–Ω—è–µ—Ç —Ñ–æ–±–∏—é –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ –±–æ—è–∑–Ω—å –±–∞–Ω–∞–Ω–æ–≤", "–ó–∞–º–µ–Ω—è–µ—Ç –±–∞–≥–∞–∂ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ –∑–µ–ª—ë–Ω–æ–µ –ø–∞–ª—å—Ç–æ", "–í–æ—Ä—É–µ—Ç –±–∞–≥–∞–∂ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–ú–µ–Ω—è–µ—Ç—Å—è –±–∞–≥–∞–∂–æ–º —Å –≤—ã–±—Ä–∞–Ω–Ω—ã–º –∏–≥—Ä–æ–∫–æ–º", "–ó–Ω–∞–µ—Ç –≥–¥–µ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è —Ç–∞–π–Ω–∏–∫ —Å —Ç—É—à—ë–Ω–∫–æ–π", "–†–∞—Å–∫—Ä—ã–≤–∞–µ—Ç –±–∞–≥–∞–∂ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–†–∞—Å–∫—Ä—ã–≤–∞–µ—Ç –∑–¥–æ—Ä–æ–≤—å–µ –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞", "–ö–∞—Ä—Ç–∞ –¥—Ä—É–≥–∞: –≤—ã –ø–æ–ø–∞–¥–∞–µ—Ç–µ –≤ –±—É–Ω–∫–µ—Ä —Ç–æ–ª—å–∫–æ —Å —á–µ–ª–æ–≤–µ–∫–æ–º –Ω–∞–¥ –≤–∞–º–∏", "–ö–∞—Ä—Ç–∞ –≤—Ä–∞–≥–∞: –≤—ã –Ω–µ –ø–æ–ø–∞–¥—ë—Ç–µ –≤ –±—É–Ω–∫–µ—Ä —Å —á–µ–ª–æ–≤–µ–∫–æ–º –ø–æ–¥ –≤–∞–º–∏", "–ú–µ–Ω—è–µ—Ç –ø–æ–ª –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ –ø—Ä–æ—Ç–∏–≤–æ–ø–æ–ª–æ–∂–Ω—ã–π", "–ó–∞—Å—Ç–∞–≤–ª—è–µ—Ç –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –ª—é–±—É—é –∫–∞—Ä—Ç—É –¥–µ–π—Å—Ç–≤–∏—è"]
        self.age = (randint(18, 100))
        self.sex = choice(["–ú—É–∂—á–∏–Ω–∞", "–ñ–µ–Ω—â–∏–Ω–∞"])
        if randint(0, 150) > 135:
            self.sex += ", —á–∞–π–ª–¥—Ñ—Ä–∏"
        self.job = choice(self.JOBS)
        self.health = (choice(self.HEALTHS), randint(0, 100))
        self.fear = choice(self.FEARS)
        self.hobby = choice(self.HOBBIES)
        self.luggage = choice(self.LUGGAGES)
        self.character = choice(self.CHARACTERS)
        self.info = choice(self.INFOS)
        self.card1 = choice(self.CARDS)
        self.card2 = choice(self.CARDS)


class Shelter:
    def __init__(self):

        self.EQUIPMENT = ["–ë–∏–±–ª–∏–æ—Ç–µ–∫–∞", "–ì–µ–Ω–µ—Ä–∞—Ç–æ—Ä", "–ú–∞—Å—Ç–µ—Ä—Å–∫–∞—è", "–Ø—â–∏–∫–∏ —Å –ø—Ä–æ–¥–æ–≤–æ–ª—å—Å—Ç–≤–∏–µ–º", "–°–∫–ª–∞–¥ —Å –æ—Ä—É–∂–∏–µ–º", "–ö—Ä—ã—Å—ã", "–¢–∞—Ä–∞–∫–∞–Ω—ã", "–ü–æ—Ä–Ω–æ –∂—É—Ä–Ω–∞–ª—ã", "–°—Ç–æ–ª–æ–≤–∞—è", "–ö–ª–µ—Ç–∫–∞ –¥–ª—è –∂–∏–≤–æ—Ç–Ω—ã—Ö", "–¢–µ–ø–ª–∏—Ü–∞", "–•–æ–ª–æ–¥–∏–ª—å–Ω–∏–∫", "–ö—É—Ö–Ω—è", "–•–∏–º. –ª–∞–±–æ—Ä–∞—Ç–∏—è", "–°–∫–ª–∞–¥ —Ç—ë–ø–ª–æ–π –æ–¥–µ–∂–¥—ã", "–ú–µ–¥. –∫–∞–±–∏–Ω–µ—Ç", "–¢—Ä–µ–Ω–∞–∂—ë—Ä–Ω—ã–π –∑–∞–ª", "–ö–æ–ª–æ–¥–∞ –∫–∞—Ä—Ç", "–°—Ç—É–¥–∏—è –∑–≤—É–∫–æ–∑–∞–ø–∏—Å–∏", "–ö–æ–º–ø–ª–µ–∫—Ç –Ω–∞—Å—Ç–æ–ª—å–Ω—ã—Ö –∏–≥—Ä"]

        self.size = str(randint(50, 470)) + " –∫–≤. –º"
        self.time_to_stay = str(randint(3, 60)) + " –º–µ—Å."
        self.equipment = choice(self.EQUIPMENT)


class Apocalypse:
    def __init__(self):
        self.STORIES = ["–ó–æ–º–±–∏ - –∞–ø–æ–∫–∞–ª–∏–ø—Å–∏—Å", "–ì–ª–æ–±–∞–ª—å–Ω–æ–µ –ø–æ—Ç–µ–ø–ª–µ–Ω–∏–µ", "–õ–µ–¥–Ω–µ–∫–æ–≤—ã–π –ø–µ—Ä–∏–æ–¥", "–ü–æ—Ç–æ–ø", "–ü–æ—è–≤–∏–ª—Å—è –Ω–æ–≤—ã–π —Å–º–µ—Ä—Ç–µ–ª—å—ã–Ω–π –≤–∏—Ä—É—Å", "–ò–∑–≤–µ—Ä–∂–µ–Ω–∏–µ —Å—É–ø–µ—Ä-–≤—É–ª–∫–∞–Ω–∞", "–ü–∞–¥–µ–Ω–∏–µ –º–µ—Ç–µ–æ—Ä–∏—Ç–∞", "–Ø–¥–µ—Ä–Ω—è –≤–æ–π–Ω–∞", "–ù–∞–ø–∞–¥–µ–Ω–∏–µ –∏–Ω–æ–ø–ª–∞–Ω–µ—Ç—è–Ω", "–í–æ—Å—Å—Ç–∞–Ω–∏–µ –º–∞—à–∏–Ω", "–°—Ö–æ–∂–¥–µ–Ω–∏–µ –ó–µ–º–ª–∏ —Å –æ—Ä–±–∏—Ç—ã", "–í–∑—Ä—ã–≤ –°–æ–ª–Ω—Ü–∞", "–ü—Ä–∏—Ä–æ–¥–∞ –≤–æ—Å—Å—Ç–∞—ë—Ç –ø—Ä–æ—Ç–∏–≤ —á–µ–ª–æ–≤–µ–∫–∞", "–í–æ–∑–≤—Ä–∞—â–µ–Ω–∏–µ –≤–Ω—É–∫–∞ –ì–∏—Ç–ª–µ—Ä–∞", "–ß—É–º–∞", "–°–∏–º–∏–∞–Ω—Å–∫–∏–π –≥—Ä–∏–ø–ø", "–ë–∏–ª–ª –ì–µ–π—Ç—Å —á–∏–ø–∏—Ä—É–µ—Ç –≤—Å—ë —á–µ–ª–æ–≤–µ—á–µ—Å—Ç–≤–æ –∏ –ø—Ä–µ–≤—Ä–∞—â–∞–µ—Ç –≤ —Å–≤–æ–∏—Ö –º–∞—Ä–∏–æ–Ω–µ—Ç–æ–∫"]
        
        self.survived = str(randint(1, 33)) + "%"
        self.story = choice(self.STORIES)


class MyBot(discord.Client):
    async def on_ready(self):
        print("–í–°–Å –ó–ê–®–ò–ë–ò–°–¨! –†–ê–ë–û–¢–ê–ï–ú\n")

    async def on_message(self, message):
        if str(message.channel) == "bunkerbot" and message.content.lower() == "–¥–∞–π –ø–µ—Ä—Å–æ–Ω–∞–∂–∞":
            self.person = Person()
            await message.author.send("üë´–ü–æ–ª, –≤–æ–∑—Ä–∞—Å—Ç: " + self.person.sex + " " + str(self.person.age) + "\nüí∞–ü—Ä–æ—Ñ–µ—Å—Å–∏—è: " +
                                      self.person.job + "\nüíä–°–æ—Å—Ç–æ—è–Ω–∏–µ –∑–¥–æ—Ä–æ–≤—å—è: " +
                                      self.person.health[0] + " (" + str(self.person.health[1]) + "%)\nüò±–§–æ–±–∏—è: " +
                                      self.person.fear + "\nüé∏–•–æ–±–±–∏: " +
                                      self.person.hobby + "\nüíº–ë–∞–≥–∞–∂: " +
                                      self.person.luggage + "\nü§°–•–∞—Ä–∞–∫—Ç–µ—Ä: " +
                                      self.person.character + "\nüîç–î–æ–ø. –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è: " +
                                      self.person.info)
            await message.author.send("üÉè–ö–∞—Ä—Ç–∞ ‚Ññ1: " + self.person.card1 + "\nüé¥–ö–∞—Ä—Ç–∞ ‚Ññ2: " + self.person.card2)
        elif str(message.channel) == "bunkerbot" and message.content.lower().split()[0] == "—Å–º–µ–Ω–∞":
            self.person = Person()
            if message.content.lower().split()[1] == "–∑–¥–æ—Ä–æ–≤—å—è":
                await message.author.send("üíä–°–æ—Å—Ç–æ—è–Ω–∏–µ –∑–¥–æ—Ä–æ–≤—å—è: " + self.person.health[0] + " (" + str(self.person.health[1]) + "%)")

            elif message.content.lower().split()[1] == "–ø—Ä–æ—Ñ–µ—Å—Å–∏–∏":
                await message.author.send("üí∞–ü—Ä–æ—Ñ–µ—Å—Å–∏—è: " + self.person.job)

            elif message.content.lower().split()[1] == "—Ö–æ–±–±–∏":
                await message.author.send("üé∏–•–æ–±–±–∏: " + self.person.hobby)
                
            elif message.content.lower().split()[1] == "–±–∞–≥–∞–∂–∞":
                await message.author.send("üíº–ë–∞–≥–∞–∂: " + self.person.luggage)

        elif str(message.channel) == "bunkerbot" and message.content.lower() == "—É–¥–∞–ª–∏—Ç—å":
            async for mess in message.channel.history(limit=50):
                if not(mess.pinned):
                    await mess.delete()
                
        elif str(message.channel) == "bunkerbot" and message.content.lower() == "—É–¥–∞–ª–∏—Ç—å –ª—Å":
            async for mess in message.author.history(limit=50):
                if mess.author != message.author:
                    await mess.delete()
                    
        elif str(message.channel) == "bunkerbot" and message.content.lower() == "–∏—Å—Ç–æ—Ä–∏—è":
            self.cat = Apocalypse()
            self.shelter = Shelter()
            await message.channel.send("üåã–ö–∞—Ç–∞—Å—Ç—Ä–æ—Ñ–∞: " + self.cat.story +
                                       "\nüòé–í—ã–∂–∏–≤—à–∏—Ö " + self.cat.survived)
            await message.channel.send("üìè–†–∞–∑–º–µ—Ä –±—É–Ω–∫–µ—Ä–∞: " + self.shelter.size +
                                       "\n‚è±–î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –ø—Ä–µ–±—ã–≤–∞–Ω–∏—è: " + self.shelter.time_to_stay +
                                       "\n‚ö∞Ô∏è–í –±—É–Ω–∫–µ—Ä–µ –µ—Å—Ç—å: " + self.shelter.equipment)
client = MyBot()
client.run("*—Ç–æ–∫–µ–Ω*")
